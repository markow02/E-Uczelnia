<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:University.Views"
                    xmlns:viewmodels="clr-namespace:University.ViewModels;assembly=University.ViewModels">
    <DataTemplate DataType="{x:Type viewmodels:StudentsViewModel}">
        <views:StudentsView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:ClassroomsViewModel}">
        <views:ClassroomsView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:EnrollmentViewModel}">
        <views:EnrollmentView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:SubjectsViewModel}">
        <views:SubjectsView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:AddStudentViewModel}">
        <views:AddStudentView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:AddClassroomViewModel}">
        <views:AddClassroomView />
