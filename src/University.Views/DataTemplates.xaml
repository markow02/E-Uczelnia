    <DataTemplate DataType="{x:Type viewmodels:EditStudentViewModel}">
        <views:EditStudentView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:EditClassroomViewModel}">
        <views:EditClassroomView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:AddSubjectViewModel}">
        <views:AddSubjectView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:EditSubjectViewModel}">
        <views:EditSubjectView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:SearchViewModel}">
        <views:SearchView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:AddEnrollmentViewModel}">
        <views:AddEnrollmentView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:EditEnrollmentViewModel}">
        <views:EditEnrollmentView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:GradeViewModel}">
        <views:GradeView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:EditGradeViewModel}">
        <views:EditGradeView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:AddGradeViewModel}">
        <views:AddGradeView />
    </DataTemplate>
</ResourceDictionary>